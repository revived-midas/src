    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'ord'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=ord"
                ],
                "filter": {
                    "name": "ord",
                    "kind": "lib"
                }
            },
            "args": [
                "inscribe_over_max_standard_tx_weight"
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                // "RUST_BACKTRACE": "full",
            }
        },
